[
  {
    "image": "nplutt/ssm-bastion",
    "name": "ssm-bastion",
    "essential": true,
    "networkMode": "awsvpc",
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/ssh-bastion",
        "awslogs-region": "us-west-2",
        "awslogs-stream-prefix": "ecs"
      }
    },
    "environment": [
      {
        "name": "AUTHORIZED_SSH_KEYS",
        "value": ""
      },
      {
        "name": "ACTIVATION_CODE",
        "value": ""
      },
      {
        "name": "ACTIVATION_ID",
        "value": ""
      }
    ]
  }
]
